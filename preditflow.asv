function [flow] = preditflow(finalpath,i,x,xpath)
%UNTITLED 此处显示有关此函数的摘要
%   此处显示详细说明
tempx=x;tempxpath=xpath;
tempxpath(1:size(finalpath,1),2*i-1:2*i)=finalpath;
% temppn=pn;
% tempxorder(:,2*i-1:2*i)=zeros(1500,2);
% tempxorder(1:size(finalpath,1),2*i-1:2*i)=finalpath;
% tempxstate(i)=1;
% f=zeros(1,9);
%         tempn(i)=1;
%         tempxpath(1:size(finalpath,1),2*i-1:2*i)=finalpath;
%          tempxpoint(:,2*i-1:2*i)=[point;zeros(3-size(point,1),2)];
%          temppn(i)=size(point,1);
% worktime=zeros(1,100);dt=0.1;         
% while 1
% for ii=1:50
% [tempx,tempxstate,tempstate,tempxpath,tempn,temppn,tempxorder] = Executetask(tempx,tempxstate,tempstate,tempxorder,tempxpath,tempxpoint,tempn,temppn,crash,target,stopp,trajp,path,dis);
